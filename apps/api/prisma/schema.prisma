
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id        String       @id @default(cuid())
  email     String       @unique
  password  String
  files     FileObject[]
  createdAt DateTime     @default(now())
}

model FileObject {
  id        String   @id @default(cuid())
  key       String   @unique
  ownerId   String?
  owner     User?    @relation(fields: [ownerId], references: [id], onDelete: SetNull)
  createdAt DateTime @default(now())
}
